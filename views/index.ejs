<html> 	<head>		<title>Safe Door Open</title>		<script src='https://code.jquery.com/jquery-3.1.1.min.js'></script>		<script src='/socket.io/socket.io.js'></script>		<script>			$(function() {				var socket = io();				var id = 'Guest..' + Math.floor((Math.random() * 10) + 1);				socket.on('msg', function(data) {					$('#msgs').append($('<p>').text('ID: ' + data.id + ': ' + data.msg));				});				var sendMsg = function(type) {					var msg = $('input').val();					$('#msgs').append($('<p>').text('ME: ' + id + ': ' + msg));					socket.emit(type, { id: id, msg: msg});					$('input').val('');				}				$('#close').click(function() {					sendMsg('close');				});				$('#open').click(function() {					sendMsg('open');				});			});		</script>	</head>	<body>		<div id="door_state">			<label> 도어락 제어</label>			<button id='open'>open</button>			<button id='close'>close</button>		</div>		<div id="state_record">			<form action="./record" method="post">				<label> 시작 날짜</label>				<input type="date" name="start_date">				<label> 종료 날짜</label>				<input type="date" name="end_date">				<input type="submit" value="도어락 기록 보기">			</form>		</div>w		<div id="door_camera">			<iframe src="http://192.168.137.177:8000" width="1000" height="600">		</div>	</body></html>